    "           \"l2_leaf_reg\":(0,10),\n",
    "           \"model_size_reg\": (0,10)\n",
    "}\n",
    "optimizer = BayesianOptimization(\n",
    "    f = CB_opt,\n",
    "    pbounds = pbounds,\n",
    "    verbose = 2,\n",
    "    random_state = 888,\n",
    ")\n",
    "\n",
    "optimizer.maximize(init_points = 2, n_iter = 20)\n",
